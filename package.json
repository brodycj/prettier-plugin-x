{
  "name": "prettier-plugin-x",
  "version": "0.0.5-dev",
  "description": "TBD",
  "author": "@brodybits (Christopher J. Brody) <chris@brody.consulting>",
  "license": "(MIT OR Apache-2.0)",
  "homepage": "https://github.com/brodybits/prettier-plugin-x#readme",
  "bugs": "https://github.com/brodybits/prettier-plugin-x/issues",
  "main": "index.js",
  "scripts": {
    "prettier-index": "prettier --check --trailing-comma=none index.js",
    "prettier-sample": "prettier --config ./sample-prettierx-prettierrc --plugin=. --parser=x-babel --check sample.js",
    "prettier-sample-typescript": "prettier --config ./sample-prettierx-prettierrc --plugin=. --parser=x-typescript --check sample.js",
    "lint-sample": "standard sample.js",
    "test": "npm run prettier-index && npm run lint-sample && npm run prettier-sample && npm run prettier-sample-typescript"
  },
  "peerDependencies": {
    "prettier": "^2.0.0"
  },
  "dependencies": {
    "prettierx": "^0.14.3"
  },
  "devDependencies": {
    "prettier": "^2.2.1",
    "standard": "^16.0.3"
  }
}
